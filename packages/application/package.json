{
  "name": "@yaba/application",
  "version": "1.0.0",
  "main": "app/main/index.js",
  "scripts": {
    "clean": "rm -rf dist release",
    "start:main": "PORT=8000 electron .",
    "start:main:prod": "NODE_ENV=production electron dist",
    "start:renderer": "webpack-dev-server --config webpack.config.renderer.js",
    "build:renderer:prod": "webpack -p --config webpack.config.renderer.js",
    "build:main:prod": "webpack -p --config webpack.config.main.js",
    "build:prod": "yarn run clean && yarn run build:renderer:prod && yarn run build:main:prod && cp package.release.json dist/package.json",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "license": "MIT",
  "dependencies": {
    "@yaba/analyzer": "1.0.0",
    "@yaba/icon": "1.0.0",
    "@yaba/statsParser": "1.0.0",
    "antd": "3.8.1",
    "axios": "0.18.0",
    "codemirror": "5.39.2",
    "date-fns": "1.29.0",
    "electron": "2.0.7",
    "history": "4.7.2",
    "iltorb": "2.4.0",
    "localforage": "1.7.2",
    "lodash": "4.17.10",
    "mime-types": "2.1.19",
    "node-ipc": "9.1.1",
    "path-to-regexp": "2.2.1",
    "prop-types": "15.6.2",
    "qs": "6.5.2",
    "react": "16.4.2",
    "react-codemirror2": "5.1.0",
    "react-dom": "16.4.2",
    "react-json-tree": "0.11.0",
    "react-redux": "5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-virtualized": "9.20.1",
    "redux": "4.0.0",
    "redux-devtools-extension": "^2.13.5",
    "redux-promise-middleware": "5.1.1",
    "redux-thunk": "2.3.0",
    "semver": "5.5.0",
    "universal-analytics": "^0.4.17",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@yaba/plugin": "1.0.0",
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.6",
    "babel-loader": "7.1.5",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "css-loader": "1.0.0",
    "devtron": "1.4.0",
    "electron-builder": "^20.28.1",
    "electron-devtools-installer": "2.2.4",
    "electron-rebuild": "1.8.2",
    "file-loader": "1.1.11",
    "html-webpack-plugin": "3.2.0",
    "lodash.flatten": "4.4.0",
    "node-loader": "^0.6.0",
    "style-loader": "0.22.1",
    "webpack": "4.16.5",
    "webpack-cli": "3.1.0",
    "webpack-dev-server": "3.1.5",
    "why-did-you-update": "0.1.1"
  },
  "build": {
    "appId": "com.yaba.io",
    "mac": {
      "category": "public.app-category.developer-tools",
      "target": "dmg",
      "icon": "../icon/mac/icon.icns"
    },
    "directories": {
      "output": "release",
      "app": "dist"
    }
  }
